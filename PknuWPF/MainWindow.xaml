<Window x:Class="PknuWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PknuWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Width="120">
        <TextBlock TextWrapping="Wrap" Text="TextBlock" Height="40" Margin="10"/>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox" Margin="10"/>
        <ProgressBar x:Name="PgdTest" Height="30" Margin="10" Value="60"/>
        <Slider x:Name="slider" ValueChanged="slider_ValueChanged" Margin="10" Value="3"/>
        <PasswordBox PasswordChar="P" Password="Test" Height="30" Margin="10" />


    </StackPanel>
</Window>
