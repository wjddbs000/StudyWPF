<Window x:Class="BikeShopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BikeShopApp"
        mc:Ignorable="d"
        Height="{StaticResource Pknu_Height}"
        Width="{StaticResource Pknu_Width}"
        Title="{StaticResource Pknu_Title}">
    <Grid Loaded="Grid_Loaded">
        <Frame x:Name="MainFrame" Source="/BikeShopApp;Component/Menu.xaml" NavigationUIVisibility="Visible" />
    </Grid>
</Window>
